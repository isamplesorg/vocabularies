.
essfrole:cement
  rdf:type skos:Concept ;
  dcterm:source "This vocabulary" ;
  rdfs:isDefinedBy essfrole:ontology ;
  rdfs:label "cement"@en ;
  skos:broader essfrole:individualconstituent ;
  skos:definition "sampled feature is material that binds constituent particles together in a sedimentary rock."@en ;
  skos:inScheme essfrole:ontology ;
  skos:prefLabel "cement"@en ;
.
essfrole:clast
  rdf:type skos:Concept ;
  dcterm:source "This vocabulary" ;
  rdfs:isDefinedBy essfrole:ontology ;
  rdfs:label "clast"@en ;
  skos:broader essfrole:inclusion ;
  skos:definition "A clast is a fragment of rock or mineral derived from some pre-existing rock or generated by intrabasinal processes that is a part of a sediment"@en ;
  skos:inScheme essfrole:ontology ;
  skos:prefLabel "clast"@en ;
.
essfrole:cognateinclusion
  rdf:type skos:Concept ;
  dcterm:source "This vocabulary" ;
  rdfs:isDefinedBy essfrole:ontology ;
  rdfs:label "cognate inclusion"@en ;
  skos:altLabel "autolith"@en ;
  skos:broader essfrole:inclusion ;
  skos:definition "An inclusion in an igneous rock that is interpreted to be genetically related to the enclosing rock."@en ;
  skos:inScheme essfrole:ontology ;
  skos:prefLabel "cognate inclusion"@en ;
.
essfrole:fluidinclusion
  rdf:type skos:Concept ;
  dcterm:source "This vocabulary" ;
  rdfs:isDefinedBy essfrole:ontology ;
  rdfs:label "fluid inclusion"@en ;
  skos:broader essfrole:inclusion ;
  skos:definition "an inclusion that is an opening in a mineral grain containing fluid (liquid or gas)."@en ;
  skos:inScheme essfrole:ontology ;
  skos:prefLabel "fluid inclusion"@en ;
.
essfrole:groundmass
  rdf:type skos:Concept ;
  dcterm:source "This vocabulary" ;
  rdfs:isDefinedBy essfrole:ontology ;
  rdfs:label "groundmass"@en ;
  skos:broader essfrole:individualconstituent ;
  skos:definition "sampled feature is fine-grained to aphanitic material that is interstitial to larger crystals or particles in a sample."@en ;
  skos:inScheme essfrole:ontology ;
  skos:prefLabel "groundmass"@en ;
.
essfrole:inclusion
  rdf:type skos:Concept ;
  dcterm:source "this vocabulary" ;
  rdfs:label "inclusion"@en ;
  skos:broader essfrole:sampledfeaturepart ;
  skos:definition "sampled feature is an object contained within a larger body of material."@en ;
  skos:example "fluid inclusion, xenolith, clast in a sedimentary material."@en ;
  skos:prefLabel "inclusion"@en ;
.
essfrole:individualconstituent
  rdf:type skos:Concept ;
  dcterm:source "This vocabulary" ;
  rdfs:isDefinedBy essfrole:ontology ;
  rdfs:label "individual constituent"@en ;
  skos:broader essfrole:sampledfeaturepart ;
  skos:definition "sampled feature is a single particle or collection of particles that share some characteristic, within or extracted from a larger sample."@en ;
  skos:example "mineral separate, point analysis of particular particles"@en ;
  skos:inScheme essfrole:ontology ;
  skos:prefLabel "individual constituent"@en ;
.
essfrole:meltinclusion
  rdf:type skos:Concept ;
  dcterm:source "This vocabulary" ;
  rdfs:isDefinedBy essfrole:ontology ;
  rdfs:label "melt inclusion"@en ;
  skos:broader essfrole:inclusion ;
  skos:definition "An inclusion in a mineral grain in an igneous rock that is interpreted to consist of trapped magma from which the igneous rock was formed."@en ;
  skos:inScheme essfrole:ontology ;
  skos:prefLabel "melt inclusion"@en ;
.
essfrole:mineralinclusion
  rdf:type skos:Concept ;
  dcterm:source "This vocabulary" ;
  rdfs:isDefinedBy essfrole:ontology ;
  rdfs:label "mineral inclusion"@en ;
  skos:broader essfrole:inclusion ;
  skos:definition "An inclusion of a mineral phase contained inside a grain of another mineral."@en ;
  skos:inScheme essfrole:ontology ;
  skos:prefLabel "mineral inclusion"@en ;
.
essfrole:ontology
  rdf:type owl:Ontology ;
  rdf:type skos:ConceptScheme ;
  rdf:type schema:CreativeWork ;
  dcterm:contributor <https://orcid.org/0000-0001-6041-5302> ;
  dcterm:created "2023-11-17"^^xsd:date ;
  dcterm:creator <https://orcid.org/0000-0001-6041-5302> ;
  dcterm:modified "2023-11-17"^^xsd:date ;
  dcterm:publisher "iSamples project" ;
  rdfs:label "Earth and Environmental Science sampled feature role extension"@en ;
  owl:imports dcterm: ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
  skos:definition "Terms to categorize the relation of a sampled feature to its context. In the Earth Science realm this is typically relation of sampled feature to a containing rock body or rock body part."@en ;
  skos:hasTopConcept essfrole:sampledfeaturepart ;
  skos:historyNote "2023-11-17 SMR generate to account for sample classifications in SESAR"@en ;
  skos:prefLabel "Earth and Environmental Science sampled feature role extension"@en ;
