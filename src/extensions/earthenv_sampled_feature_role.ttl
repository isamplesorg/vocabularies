<essfrole:2004562>
  rdf:type schema:Grant ;
  schema:funder <https://ror.org/04nh1dc89> ;
  schema:name "Collaborative Research: Frameworks: Internet of Samples: Toward an Interdisciplinary Cyberinfrastructure for Material Samples" ;
