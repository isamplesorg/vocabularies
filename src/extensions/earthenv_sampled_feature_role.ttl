essfrole:phenocryst
  rdf:type skos:Concept ;
  dcterm:source "This vocabulary" ;
  rdfs:isDefinedBy essfrole:ontology ;
  rdfs:label "phenocryst"@en ;
  skos:broader essfrole:individualconstituent ;
  skos:definition "Sampled feature is a crystal that is significantly larger than surrounding crystals in an igneous rock."@en ;
  skos:inScheme essfrole:ontology ;
  skos:prefLabel "phenocryst"@en ;
.
essfrole:porphyroblast
  rdf:type skos:Concept ;
  dcterm:source "This vocabulary" ;
  rdfs:isDefinedBy essfrole:ontology ;
  rdfs:label "porphyroblast"@en ;
  skos:broader essfrole:individualconstituent ;
  skos:definition "Sampled feature is a crystal that is significantly larger than surrounding crystals in a metamorphic rock."@en ;
  skos:inScheme essfrole:ontology ;
  skos:prefLabel "porphyroblast"@en ;
.
essfrole:rockbody
  rdf:type skos:Concept ;
  dcterm:source "This vocabulary" ;
  rdfs:isDefinedBy essfrole:ontology ;
  rdfs:label "rock body"@en ;
  skos:broader essfrole:sampledfeaturepart ;
  skos:definition "sampled feature is representative of a rock body, i.e. it represents the whole of the sampled body."@en ;
  skos:inScheme essfrole:ontology ;
  skos:prefLabel "rock body"@en ;
.
essfrole:sampledfeaturepart
  rdf:type skos:Concept ;
  dcterm:source "this vocabulary" ;
  rdfs:label "sampled feature part"@en ;
  skos:definition "a sample feature that is part of a larger sampled feature; Could be improper part (e.g. the whole larger feature)"@en ;
  skos:prefLabel "Resonance ionization mass spectrometry" ;
  skos:topConceptOf essfrole:ontology ;
.
essfrole:xenolith
  rdf:type skos:Concept ;
  dcterm:source "This vocabulary" ;
  rdfs:isDefinedBy essfrole:ontology ;
  rdfs:label "xenolith"@en ;
  skos:broader essfrole:inclusion ;
  skos:definition "An inclusion that is a fragment of pre-existing material that is contained in an igneous rock and unrelated to the magma that formed the rock."@en ;
  skos:inScheme essfrole:ontology ;
  skos:prefLabel "xenolith"@en ;
.
